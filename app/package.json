{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "relay": "relay-compiler --src ./src --schema ../schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "relay:watch": "relay-compiler --src ./src --schema ../schema.graphql --language typescript --artifactDirectory ./src/__generated__ --watch"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-react": "^7.12.5",
    "@types/relay-runtime": "^10.1.0",
    "autoprefixer": "^10.0.2",
    "date-fns": "^2.16.1",
    "postcss": "^8.1.7",
    "react": "^0.0.0-experimental-4ead6b530",
    "react-beautiful-dnd": "^13.0.0",
    "react-day-picker": "^7.4.8",
    "react-dom": "^0.0.0-experimental-4ead6b530",
    "react-relay": "experimental",
    "relay-runtime": "^10.1.0",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@snowpack/plugin-babel": "^2.1.4",
    "@snowpack/plugin-build-script": "^2.0.11",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-react-refresh": "^2.3.7",
    "@snowpack/plugin-run-script": "^2.2.0",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/web-test-runner-plugin": "^0.1.4",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/date-fns": "^2.6.0",
    "@types/node": "^14.14.8",
    "@types/react": "^16.9.56",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.9.7",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "chai": "^4.2.0",
    "graphql": "^15.4.0",
    "postcss-cli": "^8.3.0",
    "prettier": "^2.0.5",
    "relay-compiler-language-typescript": "^13.0.2",
    "snowpack": "^2.17.1",
    "typescript": "^4.0.0"
  }
}
